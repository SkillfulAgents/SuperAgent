{
  "name": "superagent",
  "version": "0.1.1-rc2",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm rebuild better-sqlite3 && vite",
    "dev:web": "vite",
    "dev:api": "tsx watch src/web/server.ts",
    "dev:electron": "electron-rebuild -f && electron-vite dev",
    "build": "npm run build:web && npm run build:api",
    "build:web": "vite build",
    "build:api": "tsup",
    "build:electron": "electron-vite build",
    "preview": "npm run build && node dist/web/server.js",
    "dist:mac": "electron-vite build && electron-builder --mac",
    "dist:win": "electron-vite build && electron-builder --win",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:container": "npm --prefix agent-container test",
    "test:e2e": "E2E_MOCK=true playwright test --project=web-chromium",
    "test:e2e:headed": "E2E_MOCK=true playwright test --project=web-chromium --headed",
    "test:e2e:ui": "E2E_MOCK=true playwright test --ui",
    "test:e2e:electron": "npm run build:electron && E2E_MOCK=true playwright test --project=electron",
    "db:reset": "rm superagent.db* && drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "build:container": "docker build -t superagent-container ./agent-container"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@hono/node-server": "^1.13.7",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.56.0",
    "better-sqlite3": "^11.3.0",
    "chrono-node": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.5.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.33.0",
    "electron-updater": "^6.7.3",
    "eventsource": "^4.1.0",
    "hono": "^4.6.14",
    "lucide-react": "^0.446.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@hono/vite-dev-server": "^0.24.1",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.24.0",
    "electron": "^33.2.1",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.3.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.18"
  },
  "build": {
    "appId": "com.superagent.app",
    "productName": "Superagent",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      {
        "from": "src/shared/lib/db/migrations",
        "to": "migrations"
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.icon",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "notarize": true
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "SkillfulAgents",
      "repo": "SuperAgent"
    }
  }
}
